    "@liquality/ethereum-js-wallet-provider": "1.1.8",
    "@liquality/ethereum-networks": "1.4.3",
    "@liquality/ethereum-rpc-fee-provider": "1.1.8",
    "@liquality/ethereum-rpc-provider": "1.1.8",
    "@liquality/ethereum-scraper-swap-find-provider": "1.1.8",
    "@liquality/ethereum-swap-provider": "1.4.3",
    "@liquality/near-js-wallet-provider": "^1.6.1",
